{
  "name": "material-ui-advanced-table",
  "version": "0.1.0",
  "description": "Datatable for React based on https://material-ui.com/api/table/ with additional features",
  "homepage": "https://github.com/gunn4r/material-ui-advanced-table#readme",
  "main": "dist/index.js",
  "umd:main": "dist/mylib.umd.production.js",
  "module": "dist/mylib.es.production.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "test": "tsdx test --env=jsdom",
    "test:watch": "npm run test -- --watchAll",
    "start": "cross-env NODE_ENV=development tsdx watch",
    "prebuild": "rimraf dist",
    "build": "cross-env NODE_ENV=production tsdx build",
    "prepublish": "npm run build"
  },
  "devDependencies": {
    "@babel/core": "7.4.5",
    "@types/jest": "^24.0.14",
    "@types/react": "^16.8.20",
    "@types/react-dom": "^16.8.4",
    "@typescript-eslint/eslint-plugin": "^1.10.2",
    "@typescript-eslint/parser": "^1.10.2",
    "confusing-browser-globals": "^1.0.7",
    "cross-env": "^5.2.0",
    "eslint": "5.16.0",
    "eslint-config-prettier": "^4.3.0",
    "eslint-plugin-import": "2.17.3",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-react": "7.13.0",
    "eslint-plugin-react-hooks": "^1.6.0",
    "husky": "^2.4.1",
    "lint-staged": "^8.2.1",
    "prettier": "^1.18.2",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "rimraf": "^2.6.3",
    "tsdx": "^0.6.1",
    "tslib": "^1.10.0",
    "typescript": "3.5.2"
  },
  "dependencies": {
    "@date-io/date-fns": "^1.3.6",
    "@material-ui/core": "^4.1.1",
    "@material-ui/pickers": "^3.1.0",
    "classnames": "^2.2.6",
    "date-fns": "^2.0.0-alpha.27",
    "debounce": "^1.2.0",
    "filefy": "0.1.9",
    "prop-types": "^15.7.2",
    "react-beautiful-dnd": "11.0.4",
    "react-double-scrollbar": "0.0.15"
  },
  "peerDependencies": {
    "react": ">= 16.8.5"
  },
  "lint-staged": {
    "operator/**/*.{js,jsx,ts,tsx}": [
      "eslint --ext .js,.jsx,.ts,.tsx --fix --rule 'react-hooks/exhaustive-deps: off'",
      "git add"
    ],
    "ignore": [
      "**/dist/**"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gunn4r/material-ui-advanced-table.git"
  },
  "keywords": [
    "react",
    "material-ui",
    "material",
    "datatable",
    "table"
  ],
  "author": "Gunnar Gabrielson",
  "contributors": [
    "Mehmet Baran"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/gunn4r/material-ui-advanced-table/issues"
  }
}
